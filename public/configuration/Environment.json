{
  "api": {
    "license": "configuration/api_license.json",
    "retries": 1
  },
  "subscriber_content": {
    "themes": [],
    "app": {
      "key_component": "app_router",
      "enabled": true,
      "content": {
        "functions": {
          "mount": ["function.environment.subscribe"],
          "unmount": ["function.environment.unsubscribe"]
        },
        "children": [
          {
            "key_component": "loader",
            "enabled": true,
            "loadable": true,
            "content": {
              "functions": {
                "mount": ["function.loader.subscribe"],
                "unmount": ["function.loader.unsubscribe"],
                "lifecycle": []
              }
            }
          },
          {
            "key_component": "modal_pseudo",
            "enabled": true,
            "loadable": true,
            "content": {
              "functions": {
                "mount": [
                  "function.common.subscribe",
                  "function.modal.subscribe"
                ],
                "unmount": [
                  "function.common.unsubscribe",
                  "function.modal.unsubscribe"
                ]
              }
            }
          },
          {
            "key_component": "page",
            "enabled": true,
            "loadable": false,
            "content": {
              "key_page": "Home",
              "functions": {
                "mount": [
                  "function.common.subscribe",
                  "function.navigation.page.subscribe"
                ],
                "unmount": [
                  "function.common.unsubscribe",
                  "function.navigation.page.unsubscribe"
                ]
              },
              "children": [
                {
                  "key_component": "text",
                  "enabled": true,
                  "loadable": true,
                  "content": {
                    "functions": {
                      "mount": ["function.common.subscribe"],
                      "unmount": ["function.common.unsubscribe"]
                    },
                    "text": "Select Markets"
                  }
                },
                {
                  "key_component": "hero",
                  "enabled": true,
                  "loadable": true,
                  "content": {
                    "functions": {
                      "mount": [
                        "function.common.subscribe",
                        "function.assets.subscribe"
                      ],
                      "unmount": [
                        "function.common.unsubscribe",
                        "function.assets.unsubscribe"
                      ]
                    },
                    "assets": [{ "key_asset": "hero" }]
                  }
                }
              ]
            }
          }
        ]
      }
    },
    "modals": [],
    "notices": [],
    "assets": [{ "key_asset": "hero", "url": "assets/models/select.stl" }]
  }
}
